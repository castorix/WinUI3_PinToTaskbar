<?xml version="1.0" encoding="utf-8"?>
<Window
    x:Class="WinUI3_PinToTaskbar.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WinUI3_PinToTaskbar"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="WinUI3_PinToTaskbar">

    <!--<StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button x:Name="myButton" Click="myButton_Click">Click Me</Button>
    </StackPanel>-->

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="60"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"></RowDefinition>
            <!--<RowDefinition Height="60"></RowDefinition>-->
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>

        <Grid.Resources>
            <local:SelectionToBoolConverter x:Key="SelectionConverter"/>
        </Grid.Resources>

        <StackPanel Orientation="Horizontal" Grid.Column="0" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10, 0, 0, 0">
            <Button x:Name="btnPinPinManager" Click="btnPinPinManager_Click" Background="MidnightBlue" >Pin with PinManager</Button>
            <Button x:Name="btnPinPinnedList" Click="btnPinPinnedList_Click" Background="MidnightBlue">Pin with PinnedList</Button>
            <Button x:Name="btnPinHook" Click="btnPinHook_Click"  Background="MidnightBlue">Pin/Unpin with Hook</Button>
            <Button x:Name="btnBrowse" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="5, 0, 0, 0" Width="36" Height="34"
                    Click="btnBrowse_Click" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch"
                    FontFamily="Segoe UI Emoji" FontSize="26" Content="&#x1F4C1;" Padding="0, -5, 0, 0" >
                <ToolTipService.ToolTip>
                    <ToolTip Content="Browse to select file" Placement="Bottom" Background="LightYellow" Foreground="Black"/>
                </ToolTipService.ToolTip>
            </Button>
            <TextBox x:Name="tbFile" Width="860" Height="34" Margin="0, 0, 0, 0"></TextBox>
        </StackPanel>
        <!--<Rectangle Grid.Column="0" Grid.Row="1" Margin="10, 0, 0, 10" Fill="Blue">
        </Rectangle>-->

        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"
                      Grid.Column="0" Grid.Row="1"  Margin="10, 0, 0, 10">
            <ListView x:Name="lvPinnedItems" ItemsSource="{x:Bind pinnedItems}"              
                  BorderThickness="1" BorderBrush="{ThemeResource SystemControlForegroundBaseMediumLowBrush}">
            <ListView.Resources>
                <SolidColorBrush x:Key="ListViewItemBackgroundSelected" Color="Blue" />
                <SolidColorBrush x:Key="ListViewItemBackgroundSelectedPointerOver" Color="Blue" />               
            </ListView.Resources>

            <ListView.HeaderTemplate>
                <DataTemplate>
                        <Grid Padding="12" Background="{ThemeResource SystemControlBackgroundBaseLowBrush}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="268"/>                          
                            <ColumnDefinition Width="560"/>
                            <ColumnDefinition Width="*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="Name" Style="{ThemeResource CaptionTextBlockStyle}"/>
                        <TextBlock Grid.Column="1" Text="AppID" Style="{ThemeResource CaptionTextBlockStyle}"/>
                        <TextBlock Grid.Column="2" Text="Target" Style="{ThemeResource CaptionTextBlockStyle}"/>
                    </Grid>
                </DataTemplate>
            </ListView.HeaderTemplate>

            <ListView.ItemTemplate>
                <DataTemplate x:Name="TableDataTemplate" x:DataType="local:PinnedItem">
                        <Grid Height="48">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="48"/>
                                <ColumnDefinition Width="220"/>
                                <ColumnDefinition Width="560"/>
                                <ColumnDefinition Width="*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Image Source="{x:Bind Icon}" MaxWidth="32" MaxHeight="32" Stretch="Fill"/>
                            <TextBlock Grid.Column="1" VerticalAlignment="Center" Style="{ThemeResource BaseTextBlockStyle}" Text="{x:Bind DisplayName}" />
                            <TextBlock Grid.Column="2" VerticalAlignment="Center" HorizontalAlignment="Left" Text="{x:Bind AppID}" TextWrapping="Wrap"/>
 
                            <TextBlock Grid.Column="3" VerticalAlignment="Center" HorizontalAlignment="Left" Text="{x:Bind LinkTarget}">
                                <ToolTipService.ToolTip>
                                    <ToolTip Content="Right-click to copy" Placement="Bottom" Background="LightYellow" Foreground="Black"/>
                                </ToolTipService.ToolTip>
                            </TextBlock>

                            <Grid.ContextFlyout>
                                <MenuFlyout>
                                    <MenuFlyoutItem Text="Copy Target" Click="CopyLinkTarget_Click"/>
                                </MenuFlyout>
                            </Grid.ContextFlyout>
                        </Grid>
                    </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
        </ScrollViewer>

